<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Departamento">

    <EditText
        android:id="@+id/editText_idEdificio"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:hint="Edificio"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/editText_fechaContrato"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editText_fechaContrato"
        app:layout_constraintTop_toBottomOf="@+id/editText_estacionamiento" />

    <EditText
        android:id="@+id/editText_estacionamiento"
        android:layout_width="194dp"
        android:layout_height="40dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:hint="Â¿Tiene Estacionamiento?"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/editText_fechaContrato"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/editText_fechaContrato"
        app:layout_constraintTop_toBottomOf="@+id/editText_fechaContrato" />

    <EditText
        android:id="@+id/editText_fechaContrato"
        android:layout_width="194dp"
        android:layout_height="40dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:hint="Fecha del Contrato"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/editText_alquiler"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editText_alquiler"
        app:layout_constraintTop_toBottomOf="@+id/editText_alquiler" />

    <EditText
        android:id="@+id/editText_alquiler"
        android:layout_width="194dp"
        android:layout_height="40dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:hint="Alquiler"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/editText_nombreInquilino"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editText_nombreInquilino"
        app:layout_constraintTop_toBottomOf="@+id/editText_nombreInquilino" />

    <EditText
        android:id="@+id/editText_nombreInquilino"
        android:layout_width="194dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Nombre Inquilino"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.178" />

    <Button
        android:id="@+id/btn_insert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="INSERTAR"
        app:layout_constraintEnd_toEndOf="@+id/editText_estacionamiento"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="@+id/editText_estacionamiento"
        app:layout_constraintTop_toBottomOf="@+id/editText_idEdificio" />

</androidx.constraintlayout.widget.ConstraintLayout>